{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:uat": "playwright test tests/uat/",
    "test:e2e": "playwright test tests/e2e/",
    "test:functional": "playwright test tests/functional/ --config=playwright.visual.config.ts",
    "test:functional:auth": "playwright test tests/functional/01-authentication.spec.ts --config=playwright.visual.config.ts",
    "test:functional:dashboard": "playwright test tests/functional/02-dashboard.spec.ts --config=playwright.visual.config.ts",
    "test:functional:tools": "playwright test tests/functional/03-coaching-tools.spec.ts --config=playwright.visual.config.ts",
    "test:functional:messaging": "playwright test tests/functional/04-messaging.spec.ts --config=playwright.visual.config.ts",
    "test:functional:blog": "playwright test tests/functional/05-blog-admin.spec.ts --config=playwright.visual.config.ts",
    "test:functional:access": "playwright test tests/functional/06-access-control.spec.ts --config=playwright.visual.config.ts",
    "test:functional:goals": "playwright test tests/functional/07-goals.spec.ts --config=playwright.visual.config.ts",
    "test:functional:sessions": "playwright test tests/functional/08-sessions.spec.ts --config=playwright.visual.config.ts",
    "test:functional:icf": "playwright test tests/functional/09-icf-simulator.spec.ts --config=playwright.visual.config.ts",
    "test:functional:public": "playwright test tests/functional/10-public-pages.spec.ts --config=playwright.visual.config.ts",
    "test:visual": "playwright test tests/e2e/visual-inspection.spec.ts --config=playwright.visual.config.ts",
    "test:all": "npm run test && npm run test:uat",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production next build"
  },
  "dependencies": {
    "@google-analytics/data": "^5.2.1",
    "@google/generative-ai": "^0.24.1",
    "@stripe/stripe-js": "^8.6.3",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "googleapis": "^168.0.0",
    "isomorphic-dompurify": "^2.35.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.263.1",
    "next": "^14.2.18",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.7",
    "stripe": "^20.2.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.49",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  }
